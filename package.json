{
	"name": "qairamapos",
	"version": "1.0.0",
	"description": "Real-time website for monitoring air quality",
	"main": "index.js",
	"scripts": {
		"test": "jest --coverage",
		"test:watch": "jest --watch",
		"test:coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"serve": "webpack-dev-server -p",
		"webpack": "webpack -d --watch",
		"start": "npm run webpack",
		"doc": "jsdoc -c jsdoc.json",
		"test:debug": "npm run test --coverage --coverageReporters=coverage/text-lcov | ./node_modules/coveralls/bin/coveralls.js",
		"cover": "jest --coverage --coverageReporters=text-lcov | coveralls"
	},
	"jest": {
		"verbose": true,
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"css",
			"scss"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"moduleNameMapper": {
			"\\.(css|less|sass|scss)$": "identity-obj-proxy"
		},
		"automock": false,
		"coveragePathIgnorePatterns": ["./build/","/node_modules/", "/coverage/"],
		"collectCoverage": true,
		"coverageReporters": ["html", "text-summary", "lcov"]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/qAIRa/qAIRaMap-OpenSource.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/qAIRa/qAIRaMap-OpenSource/issues"
	},
	"homepage": "https://github.com/qAIRa/qAIRaMap-OpenSource#readme",
	"dependencies": {
		"@babel/runtime": "^7.11.2",
		"@googlemaps/jest-mocks": "0.0.3",
		"babel-jest": "^26.0.1",
		"core-util-is": "^1.0.2",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.11.5",
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-modules-commonjs": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.5",
		"@babel/register": "^7.11.5",
		"babel-core": "^6.26.3",
		"coveralls": "^3.1.0",
		"eslint": "^7.8.1",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-plugin-import": "^2.22.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.5.4",
		"jest-cli": "^26.4.2",
		"jest-fetch-mock": "^3.0.3",
		"jsdoc": "^3.6.4"
	}
}
